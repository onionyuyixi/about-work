# Lombok can be configured to add @lombok.Generated annotations to# all generated
# nodes where possible; useful for JaCoCo (which has built in support), or other
# style checkers and code coverage tools
lombok.addLombokGeneratedAnnotation=true
#  generated setters will 'chain' by default
lombok.accessors.chain=true
lombok.copyableAnnotations += org.springframework.beans.factory.annotation.Qualifier
